{
  "name": "khufu",
  "version": "0.4.5",
  "description": "A template language for incremental-dom or DSL for javascript views",
  "keywords": ["redux", "html", "web", "hot", "incremental-dom"],
  "homepage": "http://github.com/tailhook/khufu",
  "main": "lib/index.js",
  "webpackLoader": "lib/loader.js",
  "files": ["lib", "src"],
  "dependencies": {
    "babel-core": "6.14",
    "babel-cli": "6.14",
    "babel-register": "6.14",
    "babel-types": "6.15",
    "babel-polyfill": "6.13",
    "jison": "0.4.15",
    "postcss": "5.0.14",
    "postcss-safe-parser": "1.0.7"
  },
  "devDependencies": {
    "webpack": "// Build tools",
    "webpack": "3.5.3",
    "babel-loader": "7.1.1",
    "redux": "3.7.2",
    "incremental-dom": "0.5.1",
    "rimraf": "2.6.1",

    "babel-preset-es2015": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",

    "redux-saga": "// For nice examples with saga",
    "redux-saga": "0.15.6",
    "core-js": "2.5.0",

    "webpack-dev-server": "// For hot reload, with dependencies",
    "webpack-dev-server": "2.7.1",

    "mocha": "// Test utils",
    "mocha": "*",
    "chai": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tailhook/khufu.git"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "build": "babel src --out-dir lib",
    "prepublish": "rimraf lib && npm run build"
  },
  "babel": {
    "presets": ["es2015"],
    "plugins": ["transform-object-rest-spread"]
  },
  "author": "Paul Colomiets <paul@colomiets.name>",
  "license": "(MIT OR Apache-2.0)"
}
