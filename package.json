{
  "name": "khufu",
  "version": "0.4.5",
  "description": "A template language for incremental-dom or DSL for javascript views",
  "keywords": ["redux", "html", "web", "hot", "incremental-dom"],
  "homepage": "http://github.com/tailhook/khufu",
  "main": "lib/index.js",
  "webpackLoader": "lib/loader.js",
  "files": ["lib", "src"],
  "dependencies": {
    "babel-core": "6.14",
    "babel-cli": "6.14",
    "babel-register": "6.14",
    "babel-types": "6.15",
    "babel-polyfill": "6.13",
    "jison": "0.4.15",
    "postcss": "5.0.14",
    "postcss-safe-parser": "1.0.7"
  },
  "devDependencies": {
    "webpack": "// Build tools",
    "webpack": "1.13",
    "babel-loader": "6.2",
    "redux": "3.6.0",
    "incremental-dom": "0.3.0",
    "rimraf": "2.4.3",

    "babel-preset-es2015": "6.14",
    "babel-plugin-transform-object-rest-spread": "6.8",

    "redux-saga": "// For nice examples with saga",
    "redux-saga": "0.5.0",
    "regenerator": "0.8.42",
    "babel-regenerator-runtime": "6.5",
    "core-js": "2.0.3",

    "webpack-dev-server": "// For hot reload, with dependencies",
    "webpack-dev-server": "1.14",
    "sockjs-client": "*",
    "strip-ansi": "*",
    "debug": "*",
    "inherits": "*",
    "json3": "*",
    "url-parse": "*",
    "ansi-regex": "*",
    "requires-port": "*",
    "querystringify": "*",
    "ms": "*",

    "mocha": "// Test utils",
    "mocha": "*",
    "chai": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tailhook/khufu.git"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "build": "babel src --out-dir lib",
    "prepublish": "rimraf lib && npm run build"
  },
  "babel": {
    "presets": ["es2015"],
    "plugins": ["transform-object-rest-spread"]
  },
  "author": "Paul Colomiets <paul@colomiets.name>",
  "license": "(MIT OR Apache-2.0)"
}
