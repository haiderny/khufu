import {Counter, counter, range} from './counter'
import {shit} from './counter'

view main():
  <p>
    store @counter = Counter

    <button>
       link {click} counter(1) -> @counter
       "+"

    " "
    <input disabled value=@counter>
    " "

    <button>
       link {click} counter(-1) -> @counter
       "-"

    <div>
      for x of range(3, 9):
        <button>
           link {click} counter(x) -> @counter
           "+"
           x

